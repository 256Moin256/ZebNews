<mvc:View
	controllerName=""
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
	displayBlock="true"
	height="100%"
	busyIndicatorDelay="0"
>
<Page showHeader="false">
    <FlexBox justifyContent="Center">
        <Title text="Hazrat Sheesh-Idris Alaihissalam" titleStyle="H3" class="sapUiSmallMarginTop"></Title>
    </FlexBox>
    <FlexBox justifyContent="Center">
        <Title text="Zeb News | Naushad Ahmed Razvi" titleStyle="H5"
            class="sapUiSmallMarginTop"></Title>
    </FlexBox>
    
    <FlexBox justifyContent="Center" class="sapUiMediumMarginTop">
        <Title text="Hazrat Sheesh Alaihissalam">
        </Title>
    </FlexBox>
    
    <FlexBox justifyContent="Center" class="">
        <ExpandableText
            maxCharacters="200"
            overflowMode="Popover"
            textAlign="Left"
            textDirection="LTR"
            class="sapUiSmallMarginBegin"
            text="{M1>/Ambiya_e_Kiram/Hazrat_Sheesh-Idris_Alaihissalam/0/text}">

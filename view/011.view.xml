    </FlexBox>

    <FlexBox justifyContent="Center" class="sapUiMediumMarginTop">
        <Title text="Hazrat Idris Alaihissalam">
        </Title>
    </FlexBox>
    
    <FlexBox justifyContent="Center" class="">
        <ExpandableText
            maxCharacters="200"
            overflowMode="Popover"
            textAlign="Left"
            textDirection="LTR"
            class="sapUiSmallMarginBegin"
            text="{M1>/Ambiya_e_Kiram/Hazrat_Sheesh-Idris_Alaihissalam/1/text}">
        </ExpandableText>
    </FlexBox>
    <footer>
        <c:Fragment fragmentName="zebnews.view.power" type="XML" />
    </footer>
